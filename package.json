{
  "name": "appTest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "echo 'No tests!'",
    "lint": "eslint ./",
    "quality-verify": "yarn lint && yarn test",
    "rebuild": "yarn clean && yarn build",
    "clean": "yarn clean:android && yarn clean:ios && rm -rf node_modules",
    "clean:android": "rm -rf android/app/build && rm -rf android/build && rm -rf android/.gradle && rm -rf android/build.gradle-e",
    "clean:ios": "rm -rf ios/build && rm -rf ios/Pods && rm -rf ios/appTest.xcworkspace",
    "build": "yarn && yarn build:android && yarn build:ios",
    "build:android": "npx jetifier",
    "build:ios": "cd ios/ && pod install",
    "android": "react-native run-android --variant=devDebug",
    "android:release": "react-native run-android --variant=prodRelease --no-packager",
    "android:qa": "react-native run-android --variant=qaRelease --no-packager",
    "ios": "react-native run-ios --scheme appTest"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.6",
    "@react-navigation/bottom-tabs": "^5.1.0",
    "@react-navigation/drawer": "^5.1.1",
    "@react-navigation/native": "^5.0.8",
    "@react-navigation/stack": "^5.1.0",
    "axios": "^0.19.2",
    "fsevents": "^2.1.2",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-gesture-bottom-sheet": "^1.0.1",
    "react-native-gesture-handler": "^1.6.0",
    "react-native-maps": "^0.26.1",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.7.3",
    "react-native-screens": "^2.2.0",
    "react-native-status-bar-height": "^2.4.0",
    "react-native-vector-icons": "^6.6.0",
    "react-native-webview": "^8.1.2",
    "react-native-youtube": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier": "1.19.1",
    "react-test-renderer": "16.9.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
